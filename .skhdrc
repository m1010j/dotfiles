# enter fullscreen mode for the focused container
cmd + alt - f : chunkc tiling::window --toggle fullscreen
# change focus between tiling / floating windows
cmd + shift + alt - space : chunkc tiling::window --toggle float
# change layout of desktop
cmd + alt - e : chunkc tiling::desktop --layout bsp
cmd + alt - s : chunkc tiling::desktop --layout monocle
# kill focused window
cmd + shift + alt - q : chunkc tiling::window --close
# change focus
cmd + alt - left : chunkc tiling::window --focus west
cmd + alt - down : chunkc tiling::window --focus south
cmd + alt - up : chunkc tiling::window --focus north
cmd + alt - right : chunkc tiling::window --focus east
cmd + alt - p : chunkc tiling::window --focus prev
cmd + alt - n : chunkc tiling::window --focus next
# move focused window
cmd + shift + alt - left : chunkc tiling::window --warp west
cmd + shift + alt - down : chunkc tiling::window --warp south
cmd + shift + alt - up : chunkc tiling::window --warp north
cmd + shift + alt - right : chunkc tiling::window --warp east
cmd + alt - r : chunkc tiling::desktop --rotate 90
# move focused container to workspace
cmd + shift + alt - m : chunkc tiling::window --send-to-desktop $(chunkc get _last_active_desktop)
cmd + shift + alt - p : chunkc tiling::window --send-to-desktop prev
cmd + shift + alt - n : chunkc tiling::window --send-to-desktop next
cmd + shift + alt - 1 : chunkc tiling::window --send-to-desktop 1
cmd + shift + alt - 2 : chunkc tiling::window --send-to-desktop 2
cmd + shift + alt - 3 : chunkc tiling::window --send-to-desktop 3
cmd + shift + alt - 4 : chunkc tiling::window --send-to-desktop 4
cmd + shift + alt - 5 : chunkc tiling::window --send-to-desktop 5
cmd + shift + alt - 6 : chunkc tiling::window --send-to-desktop 6
cmd + shift + alt - r : brew services restart chunkwm